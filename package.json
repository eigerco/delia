{
  "name": "delia",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://jmg-duarte.github.io/delia/",
  "scripts": {
    "dev": "pnpm run build:wasm && vite",
    "build:wasm": "wasm-pack build --target web ./wasm-commp",
    "build": "pnpm run build:wasm && tsc -b && vite build",
    "preview": "vite preview",
    "lint": "biome lint ./src && biome format ./src && tsc",
    "fmt": "biome format --write ./src",
    "fmt-check": "biome format ./src",
    "prepare": "husky",
    "wasm-test": "wasm-pack test --node ./wasm-commp",
    "papi:add": "pnpm exec papi add polkaStorage -w ${PAPI_ENDPOINT:-ws://localhost:42069}",
    "papi:generate": "papi generate",
    "papi": "pnpm run papi:add && pnpm run papi:generate"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@ipld/car": "^5.4.0",
    "@ipld/dag-pb": "^4.1.3",
    "@multiformats/multiaddr": "^12.4.0",
    "@polkadot-api/descriptors": "file:.papi/descriptors",
    "@polkadot/api": "^15.7.2",
    "@polkadot/extension-dapp": "^0.58.4",
    "@polkadot/extension-inject": "^0.58.4",
    "@polkadot/util": "^13.4.3",
    "@polkadot/util-crypto": "^13.4.3",
    "@tailwindcss/vite": "^4.0.9",
    "autoprefixer": "^10.4.20",
    "ipfs-unixfs": "^11.2.1",
    "it-cbor-stream": "^1.0.1",
    "lucide-react": "^0.477.0",
    "multiformats": "^13.3.2",
    "polkadot-api": "^1.14.1",
    "postcss": "^8.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.55.0",
    "react-hot-toast": "^2.5.2",
    "react-router": "^7.4.0",
    "react-tooltip": "^5.28.0",
    "tailwindcss": "^4.0.9",
    "uint8-varint": "^2.0.4",
    "wasm-commp": "link:wasm-commp/pkg",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@polkadot/types": "^15.7.2",
    "@polkadot/types-codec": "^15.8.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  }
}
